    <OctopusProperties isActive="True" previewMode="False" leftIcon="cloneRevolve.png" rightIcon="None" iconOnly="False" buttonText="Revolve">
      <ActionData active="True" type="0" XIntensity="1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
        <Command></Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
      <ActionData active="True" type="1" XIntensity="0.1" YIntensity="0.1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
        <Command>axis = {4}
startPos = $.pos
objs = #()
for i = 1 to {2} do (
	obj = instance $
	obj.wirecolor = color 255 127 0
	append objs obj
)

objsCount = objs.count

theCenter = $.pos
theDir = [0,0,0]
theDir[{5}] = 1
theLocalTM = matrixFromNormal (normalize theDir)
theLocalTM.translation = theCenter

in coordSys theLocalTM
(
	theCoords = for i = 1 to objsCount collect objs[i].pos * [1,1,0] 

	theRadius = {0} 
	theDelta = 360.0 / objsCount
	theOffset = 0 
	
	for i = 1 to objsCount do
	(
		p = theCoords[i]
		theAngle = atan2 p.y p.x
		theOffset += mod (720 + theAngle - theDelta * i) 360
	)
	
	theOffset /= objsCount 
	
	for i = 1 to objsCount do
	(
		theAngle = theOffset + theDelta * i
		objs[i].pos = ((theRadius * [cos theAngle, sin theAngle, 0]))
	)
	
)

redrawViews()</Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
