        <Command>axis = {3}
startPos = $.pos
objs = #()
for i = 1 to {2} do (
	obj = instance $
	obj.wirecolor = color 255 127 0
	append objs obj
)

objsCount = objs.count

if {1} &lt; 0 then
(
	for i = 1 to objsCount do  (
		offset_p3 = [0,0,0]
		offset_p3[axis] = offset_p3[axis] + {0} * {6} * i
		objs[i].pos = startPos + offset_p3
	)
)
else
(
	stepping = {0} / (objsCount as float)

	for i = 1 to objsCount do  (
		offset_p3 = [0,0,0]
		offset_p3[axis] = offset_p3[axis] + stepping * {6} * i
		objs[i].pos = startPos + offset_p3
