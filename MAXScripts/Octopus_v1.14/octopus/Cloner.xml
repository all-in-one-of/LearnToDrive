    <OctopusProperties isActive="False" previewMode="False" leftIcon="None" rightIcon="None" iconOnly="False" buttonText="">
      <ActionData active="False" type="0" XIntensity="1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
        <Command></Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
      <ActionData active="True" type="1" XIntensity="0.1" YIntensity="1" XStepping="0.1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="1" WheelMax="1000">
        <Command>axis = {4}
startPos = $.pos
objs = #()
for i = 1 to {2} do (
	obj = instance $
	obj.wirecolor = color 255 127 0
	append objs obj
)

objsCount = objs.count

for i = 1 to objsCount do  (
	offset_p3 = [0,0,0]
	offset_p3[axis] = offset_p3[axis] + {0} * {7} * i
	objs[i].pos = startPos + offset_p3

)

redrawViews()</Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
    </OctopusProperties>
    <OctopusProperties isActive="True" previewMode="False" leftIcon="cloneBetween.png" rightIcon="None" iconOnly="False" buttonText="Clone B">
      <ActionData active="True" type="0" XIntensity="1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
        <Command></Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
      <ActionData active="True" type="1" XIntensity="0.1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="1" WheelMax="1000">
        <Command>-- Position base value
base = bPos - aPos
inc = base / {2} as float

-- Rotation base value
incRot = 1/{2} as float

-- Scale base value
baseScl = bScl - aScl
incScl = baseScl / {2} as float

for i = 1 to {2}-1 do 
(
	-- new object
	newObj = instance sel[2]
	newObj.wirecolor = color 255 127 0

	-- position calculation
	posNew = aPos + (inc * i)
	newObj.pos = posNew

	-- scale calculation
	sclNew = aScl + (incScl * i)
	newObj.scale = sclNew

	-- rotation calculation
	in coordsys newObj.transform
	(
		r = slerp sel[1].rotation sel[2].rotation (incRot * i)
		newObj.rotation = r
	)
)

redrawViews()</Command>
        <CommandPre>sel = $selection

aPos = sel[1].pos
bPos = sel[2].pos

aScl = sel[1].scale
bScl = sel[2].scale</CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
    </OctopusProperties>
    <OctopusProperties isActive="False" previewMode="False" leftIcon="None" rightIcon="None" iconOnly="False" buttonText="">
      <ActionData active="True" type="0" XIntensity="1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
