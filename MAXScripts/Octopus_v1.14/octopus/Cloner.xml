    </OctopusProperties>
    <OctopusProperties isActive="True" previewMode="False" leftIcon="CloneOnSpline.png" rightIcon="None" iconOnly="False" buttonText="S Clone">
      <ActionData active="True" type="0" XIntensity="1" YIntensity="1" XStepping="1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="-1000" WheelMax="1000">
        <Command>sel = $selection

for i in sel do (
	p = i.transform
	bp = Position_XYZ()
	i.pos.controller = bp
	i.transform = p
)</Command>
        <CommandPre></CommandPre>
        <CommandPost></CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
      <ActionData active="True" type="1" XIntensity="0.1" YIntensity="1" XStepping="0.1" YStepping="1" isXAdaptive="True" isYAdaptive="True" XMin="-100000" XMax="100000" YMin="-100000" YMax="100000" WheelMin="1" WheelMax="1000">
        <Command>for i = 0 to ({2}) do (
	
	pth = path()
	pth.follow = on
	pth.path = spl

	if {1} &gt; 0 then (
		pth.bank = on
	)
	else
	(
		pth.bank = off
	)

	if {0} &gt; 0 then (
		pth.constantVel = on
	)
	else
	(
		pth.constantVel = off
	)

	dst = 100.0 / ({2}-0.0)

	pth.percent= dst * i

	obj = instance sel[2]
	obj.pos.controller = pth
	
	obj.wirecolor = color 255 127 0
)

redrawViews()</Command>
        <CommandPre>sel = $selection
spl = sel[1]
suspendEditing()
</CommandPre>
        <CommandPost>resumeEditing()</CommandPost>
        <CommandIsChecked></CommandIsChecked>
      </ActionData>
